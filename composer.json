{
    "name": "laravel/laravel",
    "type": "project",
    "description": " sorbona shop ",
    "keywords": ["laravel", "FOXiK","sfdevelop"],
    "develop": ["info@sfdevelop.com"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "artesaos/seotools": "^1.2",
        "astrotomic/laravel-translatable": "^11.12",
        "cviebrock/eloquent-sluggable": "^10.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "guzzlehttp/guzzle": "^7.2",
        "jackiedo/cart": "^3.0",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.24",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravolt/avatar": "^5.1",
        "livewire/livewire": "^2.12",
        "lorisleiva/laravel-actions": "^2.7",
        "mcamara/laravel-localization": "^1.8",
        "opcodesio/log-viewer": "^2.5",
        "predis/predis": "^2.2",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-query-builder": "^5.7",
        "spatie/laravel-view-models": "^1.5",
      "ext-intl": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^14.8",
        "laravel/envoy": "^2.9",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.31",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "stichoza/google-translate-php": "^5.1",
        "wulfheart/laravel-actions-ide-helper": "^0.5.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
